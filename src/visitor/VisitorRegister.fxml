<?import javafx.scene.layout.AnchorPane?>

<TabPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="visitor.VisitorRegisterController">
   <tabs>
      <Tab closable="false" text="Register">
         <content>
            <AnchorPane prefHeight="203.0" prefWidth="203.0">
               <children>
                  <Label layoutX="10.0" layoutY="10.0" text="first name:" underline="true" AnchorPane.leftAnchor="8.0" AnchorPane.rightAnchor="8.0" AnchorPane.topAnchor="10.0" />
                  <TextField fx:id="firstNameField" layoutY="36.0" promptText="first name..." AnchorPane.leftAnchor="8.0" AnchorPane.rightAnchor="8.0" AnchorPane.topAnchor="30.0" />
                  <Label layoutX="20.0" layoutY="20.0" text="last name:" underline="true" AnchorPane.leftAnchor="8.0" AnchorPane.rightAnchor="8.0" AnchorPane.topAnchor="60.0" />
                  <TextField fx:id="lastNameField" layoutX="8.0" layoutY="81.0" promptText="last name..." AnchorPane.leftAnchor="8.0" AnchorPane.rightAnchor="8.0" AnchorPane.topAnchor="80.0" />
                  <Label layoutX="8.0" layoutY="110.0" text="Phone number:" underline="true" />
                  <TextField fx:id="PhoneNrField" layoutX="8.0" layoutY="127.0" prefHeight="25.0" prefWidth="186.0" promptText="phone number..." />
                  <Label layoutX="8.0" layoutY="152.0" text="password:" underline="true" AnchorPane.leftAnchor="8.0" AnchorPane.rightAnchor="8.0" AnchorPane.topAnchor="152.0" />
                  <PasswordField fx:id="PassField" layoutX="8.0" layoutY="176.0" promptText="password..." AnchorPane.leftAnchor="8.0" AnchorPane.rightAnchor="8.0" AnchorPane.topAnchor="176.0" />
                  <Button layoutX="57.0" layoutY="161.0" mnemonicParsing="false" onAction="#registerButtonClick" text="Register" AnchorPane.bottomAnchor="8.0" AnchorPane.leftAnchor="8.0" AnchorPane.rightAnchor="8.0" AnchorPane.topAnchor="220.0" />
               </children>
            </AnchorPane>
         </content>
      </Tab>
      <Tab closable="false" text="Login">
         <content>
            <AnchorPane prefHeight="203.0" prefWidth="203.0">
               <children>
                  <Label layoutX="10.0" layoutY="10.0" text="phone number:" underline="true" AnchorPane.leftAnchor="8.0" AnchorPane.rightAnchor="8.0" AnchorPane.topAnchor="10.0" />
                  <TextField fx:id="loginPhoneNrField" layoutY="36.0" promptText="phone number..." AnchorPane.leftAnchor="8.0" AnchorPane.rightAnchor="8.0" AnchorPane.topAnchor="30.0" />
                  <Label layoutX="18.0" layoutY="121.0" text="password:" underline="true" AnchorPane.leftAnchor="8.0" AnchorPane.rightAnchor="8.0" AnchorPane.topAnchor="60.0" />
                  <PasswordField fx:id="LoginPassField" layoutX="8.0" layoutY="81.0" promptText="password..." AnchorPane.leftAnchor="8.0" AnchorPane.rightAnchor="8.0" AnchorPane.topAnchor="80.0" />
                  <Button layoutX="57.0" layoutY="161.0" mnemonicParsing="false" onAction="#loginButtonClick" text="Login" AnchorPane.bottomAnchor="8.0" AnchorPane.leftAnchor="8.0" AnchorPane.rightAnchor="8.0" AnchorPane.topAnchor="220.0" />
               </children>
            </AnchorPane>
         </content>
      </Tab>
   </tabs>
</TabPane>
